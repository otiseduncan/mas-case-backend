# === Server ===
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# === Database ===
# For local docker-compose Postgres
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/mas_case?schema=public"

# === Auth ===
JWT_SECRET="replace-with-a-long-random-string"
ACCESS_TOKEN_TTL="15m"
REFRESH_TOKEN_TTL="7d"

# === Seeding (optional) ===
# Provide a JSON array of demo users; if empty, defaults are created.
# Each item: { "email": "...", "password": "...", "role": "admin|manager|tech", "org": "MAS" }
DEMO_USERS_JSON='[]'
